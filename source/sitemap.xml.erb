<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

<% pages = sitemap.resources.find_all{ |p| p.source_file.match(/\.html/) } %>

<% pages.each do |page| %>

  <url>
    <loc><%= sitemap_entry_helper(page) %></loc>
    <priority>0.8</priority>
  </url>

<% end %>

</urlset>